{
  "name": "johnny-silverhand/education",
  "description": "",
  "keywords": ["hackaton"],
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0.0",
    "ext-pdo": "*",
    "ext-gd": "*",
    "ext-curl": "*",
    "ext-pgsql": "*",
    "ext-intl": "*",
    "ext-zip": "*",
    "yiisoft/yii2": "~2.0.32",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.1.0",
    "yiisoft/yii2-jui": "~2.0.7",
    "yiisoft/yii2-httpclient": "~2.0.0",
    "kartik-v/yii2-widget-rating": "~1.0.4",
    "kartik-v/yii2-widget-select2": "~2.2.0",
    "kartik-v/yii2-widget-fileinput": "~1.1.0",
    "kartik-v/yii2-widget-depdrop": "~1.0.6",
    "kartik-v/yii2-widget-datepicker": "~1.4.0",
    "kartik-v/yii2-field-range": "~1.3.0",
    "kartik-v/yii2-grid": "~3.3.5",
    "kartik-v/yii2-editable": "~1.7.8",
    "2amigos/yii2-ckeditor-widget": "~2.0.0",
    "borales/yii2-phone-input": "~0.3.0",
    "leandrogehlen/yii2-treegrid": "~1.1.0",
    "bower-asset/ionrangeslider": "~2.3.0",
    "devsergeev/yii2-inn-validator": "~1.0.0",
    "mpdf/mpdf": "^8.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/codeception": "~4.1",
    "codeception/verify": "~2.1.0",
    "codeception/specify": "~1.4.0",
    "codeception/module-filesystem": "~1.0.0",
    "codeception/module-yii2": "~1.1.0",
    "codeception/module-asserts": "~1.3.0",
    "symfony/browser-kit": "~5.2.0"
  },
  "config": {
    "process-timeout": 1800
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
